    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.12.13",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
